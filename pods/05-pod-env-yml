apiVersion: v1
kind: Pod
metadata:
    name: sample5
spec:
   containers:
      - name: centos
        image: centos:7
        command:
        - sleep
        - "100000"
        envFrom:
          - configMapKeyRef:
              name: sample-meta
      env:
        - name: URL
          valueFrom:
            configMapKeyRef:
               key: URL
               name: sample-meta